{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "DOM",
      "Dom.Iterable",
      "webworker"
    ],
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "resolveJsonModule": true,
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowImportingTsExtensions": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "#cds-models/*": [
        "./@cds-models/*"
      ]
    },
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noEmitOnError": false
  },
  "include": [
    "src",
    "tests",
    "srv"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "coverage",
    "tests/fixtures",
    "tests/fixtures/*",
    "tests/fixtures/*/*"
  ]
}
